      "properties": {
        "name": {
          "description": "A2A agent name used in URL paths\n\nThis name will be used in the A2A endpoint URLs: - JSON-RPC: `/a2a/agents/{name}/v1/jsonrpc` - HTTP: `/a2a/agents/{name}/v1` - Agent Card: `/a2a/agents/{name}/.well-known/agent-card.json`\n\nMust be a valid URL path segment (alphanumeric, hyphens, underscores only)",
          "type": "string"
        },
        "ref": {
          "description": "Path to the Oxy agent configuration file (relative to project root)\n\nExample: `agents/sales-assistant.agent.yml`",
          "type": "string"
        }
      }
    },
    "A2aConfig": {
